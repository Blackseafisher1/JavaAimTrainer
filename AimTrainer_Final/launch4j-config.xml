<?xml version="1.0" encoding="UTF-8"?>
<launch4jConfig>
  <dontWrapJar>false</dontWrapJar>
  <headerType>gui</headerType>
  <jar>AimTrainer.jar</jar>
  <outfile>AimTrainer.exe</outfile>
  <errTitle>AimTrainer</errTitle>
  <cmdLine></cmdLine>
  <chdir>.</chdir>
  <priority>normal</priority>
  <downloadUrl>https://adoptium.net/temurin/releases/?version=21</downloadUrl>
  <supportUrl>https://github.com</supportUrl>
  <stayAlive>false</stayAlive>
  <restartOnCrash>false</restartOnCrash>
  <classPath>
    <mainClass>com.aimtrainer.Launcher</mainClass>
    <cp>AimTrainer.jar</cp>
  </classPath>
  <jre>
    <path>jre;%JAVA_HOME%;%PATH%</path>
    <requiresJdk>false</requiresJdk>
    <requires64Bit>false</requires64Bit>
    <minVersion>21.0.0.0</minVersion>
    <initialHeapSize>512</initialHeapSize>
    <maxHeapSize>2048</maxHeapSize>
    <opt>-Djava.awt.headless=false</opt>
  </jre>
  <versionInfo>
    <fileVersion>1.0.0</fileVersion>
    <txtFileVersion>1.0.0</txtFileVersion>
    <fileDescription>Java Aim Trainer - Desktop Game</fileDescription>
    <productName>AimTrainer</productName>
    <internalName>AimTrainer</internalName>
    <originalFilename>AimTrainer.exe</originalFilename>
    <language>ENGLISH_US</language>
    <copyright>2026 Ege Karadeniz</copyright>
  </versionInfo>
  <messages>
    <startupErr>Failed to start AimTrainer</startupErr>
    <jreNotFoundErr>Java 21 or higher is required to run AimTrainer. Please download from: https://adoptium.net/</jreNotFoundErr>
    <jreVersionErr>Java 21 or higher is required to run AimTrainer.</jreVersionErr>
    <launcherErr>An error occurred while starting AimTrainer.</launcherErr>
  </messages>
</launch4jConfig>
